<Window x:Class="AISRouting.App.WPF.Views.TrackPlotWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:AISRouting.App.WPF.Views"
        xmlns:vm="clr-namespace:AISRouting.App.WPF.ViewModels"
        xmlns:scott="clr-namespace:ScottPlot.WPF;assembly=ScottPlot.WPF"
        Title="Track Comparison" Height="700" Width="1000">
    <Window.DataContext>
        <vm:TrackPlotViewModel />
    </Window.DataContext>

    <Grid>
        <scott:WpfPlot Name="PlotControl" Margin="10"/>
    </Grid>
</Window>
